<div id="top5" class="card">
  <div class="card-header">
    {{#if titlebar}}
      {{titlebar}}
    {{else}}
      Top 5 Current Results
    {{/if}}
  </div>
  <ul id="event-list" class="list-group list-group-flush">
    {{#each events}}
    <a href="/more/{{this.id}}">
      <li 
        class="list-group-item"
        data-id="{{this.id}}">
        {{this.dataValues.title}}
      </li>
      </a>
    {{/each}}
  </ul>
</div>
